      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:scoll"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="600"
      Title="welcom">

    <Grid Background="#FFF1F1F1">
